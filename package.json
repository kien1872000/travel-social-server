{
  "name": "travel-social-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "14.17.1",
    "npm": "6.14.13"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@faker-js/faker": "^6.2.0",
    "@goongmaps/goong-sdk": "^1.0.7",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.1",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/websockets": "^8.4.1",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "eth-sig-util": "^3.0.1",
    "firebase": "^9.6.8",
    "firebase-admin": "^10.0.2",
    "handlebars": "^4.7.7",
    "mongoose": "^5.13.14",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.4.1",
    "swagger-ui-express": "^4.3.0",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.5",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@decorator/(.*)": "<rootDir>/decorator/$1",
      "@dto/(.*)": "<rootDir>/dto/$1",
      "@util/(.*)": "<rootDir>/util/$1",
      "@like/(.*)": "<rootDir>/module/likes/$1",
      "@auth/(.*)": "<rootDir>/auth/$1",
      "@upload/(.*)": "<rootDir>/upload/$1",
      "@mail/(.*)": "<rootDir>/mail/$1,",
      "@connected-socket/(.*)": "<rootDir>/module/connected-sockets/$1",
      "@chat/(.*)": "<rootDir>/module/chats/$1",
      "@notification/(.*)": "<rootDir>/module/notifications/$1",
      "@hashtag/(.*)": "<rootDir>/module/hashtags/$1",
      "@comment/(.*)": "<rootDir>/module/comments/$1",
      "@following/(.*)": "<rootDir>/module/followings/$1",
      "@config/(.*)": "<rootDir>/config/$1",
      "@entity/(.*)": "<rootDir>/entity/$1",
      "@helper/(.*)": "<rootDir>/helper/$1",
      "@post/(.*)": "<rootDir>/module/posts/$1",
      "@mediaFile/(.*)": "<rootDir>/module/media-files/$1"
    }
  }
}
